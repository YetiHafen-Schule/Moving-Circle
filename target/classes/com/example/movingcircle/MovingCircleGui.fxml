<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>

<SplitPane dividerPositions="0.5" orientation="VERTICAL" prefHeight="450.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.movingcircle.MovingCircleGui">
   <items>
      <Pane fx:id="canvas" prefHeight="200.0" prefWidth="200.0">
         <children>
            <Circle fx:id="circle" fill="DODGERBLUE" layoutX="200.0" layoutY="200.0" radius="50.0" stroke="BLACK" strokeType="INSIDE" />
         </children></Pane>
      <HBox alignment="CENTER" maxHeight="-Infinity" prefHeight="30.0" prefWidth="200.0">
         <children>
            <Button mnemonicParsing="false" onAction="#handleMoveAction" text="Move" />
         </children></HBox>
   </items>
</SplitPane>
